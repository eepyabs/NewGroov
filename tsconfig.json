{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": [
      "node",
      "eslint",
      "eslint-scope",
      "estree",
      "hammerjs",
      "json-schema",
      "keyv",
      "minimatch",
      "node-fetch",
      "tough-cookie"
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "./expo-env.d.ts",
    "screens/**/*.tsx",
    "expo-env.d.ts"
  ]
}
